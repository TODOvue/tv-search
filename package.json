{
  "name": "@todovue/tv-search",
  "private": false,
  "author": "Cristhian Daza",
  "description": "TvSearch provides a fast, accessible, and fully customizable search interface for Vue 3 apps.",
  "license": "MIT",
  "version": "1.0.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/TODOvue/tv-search.git"
  },
  "bugs": {
    "url": "https://github.com/TODOvue/tv-search/issues"
  },
  "keywords": [
    "todovue",
    "front-end",
    "web",
    "vue",
    "vuejs",
    "vue-js",
    "search",
    "show-search",
    "vue-search",
    "vue-search-component"
  ],
  "exports": {
    ".": {
      "import": "./dist/tv-search.es.js",
      "require": "./dist/tv-search.cjs.js"
    },
    "./style.css": "./dist/tv-search.css",
    "./nuxt": "./nuxt.js"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "dist/*.css"
  ],
  "main": "dist/tv-search.cjs.js",
  "module": "dist/tv-search.es.js",
  "types": "dist/tv-search.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "nuxt.js"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:demo": "cp README.md public/ && cp CHANGELOG.md public/ && VITE_BUILD_TARGET=demo vite build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@todovue/tv-button": "^1.2.2"
  },
  "devDependencies": {
    "@todovue/tv-demo": "^1.2.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "sass": "^1.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.0.0"
  }
}
